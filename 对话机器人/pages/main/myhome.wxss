.bar {
  background: white;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 15px 0;
  justify-content: space-between;
  font-size: 5vw;
  color: gray;
  z-index: 9;
  background: url(http://imgstore.cdn.sogou.com/app/a/100540002/771569.jpg);
  background-size: 100% 100%;
}

.bar image {
  width: 80px;
  height: 80px;
  border-radius: 50%;
}